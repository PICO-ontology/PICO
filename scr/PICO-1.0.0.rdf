        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-03-23T05:33:24Z</dc:date>
        <rdfs:label xml:lang="en">ethical review</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PICO_0000045 -->

