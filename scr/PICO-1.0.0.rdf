    <!-- http://purl.obolibrary.org/obo/PICO_0000043 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PICO_0000043">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GSSO_003081"/>
        <obo:IAO_0000115 xml:lang="en">A practice of ehtical governance and management according to the research ethics. A data ethics review is the representative of practible ethics.</obo:IAO_0000115>
        <obo:PICO_0000004 xml:lang="en">实践伦理学</obo:PICO_0000004>
